  digitalWrite(12, HIGH);
  digitalWrite(11, HIGH);
  digitalWrite(10, HIGH);
  digitalWrite(9, HIGH);
  digitalWrite(8, HIGH);
  digitalWrite(7, HIGH);
  digitalWrite(6, HIGH);
  digitalWrite(5, HIGH);
  digitalWrite(4, HIGH);
  digitalWrite(3, HIGH);
  digitalWrite(2, HIGH);
   
   delay(x);
   
  digitalWrite(13, LOW);
  digitalWrite(12, LOW);
  digitalWrite(11, LOW);
  digitalWrite(10, LOW);
  digitalWrite(9, LOW);
  digitalWrite(8, LOW);
  digitalWrite(7, LOW);
  digitalWrite(6, LOW);
  digitalWrite(5, LOW);
  digitalWrite(4, LOW);
  digitalWrite(3, LOW);
  digitalWrite(2, LOW);   
   
   delay(x);
}

void loop(){

  while (Serial.available() == 0){
}

	int userinput = Serial.parseInt();

switch (userinput) {
	case 1:
  LEDLightShow1();
  break;
  
  	case 2:
  LEDLightShow2();
  break;
  
  	case 3:
  LEDLightShow3();
  break;
  
    case 4:
  LEDLightShow4();
  break;
 
      case 5:
  LEDLightShow5();
  break;
  
      case 6:
  LEDLightShow6();
  break;
  
      case 7:
  LEDLightShow7();
  break;
  
      case 8:
  LEDLightShow8();
  break;
  
       case 9:
  LEDLightShow9();
  break; 
  
       case 0:
  LEDLightShow0();
  break;   
  
  default:
  
  Serial.println(" Selected Feature Does Not Exist");
}
}
